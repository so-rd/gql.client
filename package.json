{
  "name": "gql.client",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview --port 3000",
    "prepare": "husky install",
    "start:cli": "cross-env BROWSER=none vite --mode test",
    "test:jest": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:cov-open": "npm run test:cov && open coverage/lcov-report/index.html",
    "test:debug": "jest --detectOpenHandles",
    "test:clean": "jest --clearCache",
    "cy:install": "cypress install",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test:e2e:dev": "start-server-and-test start:cli http-get://localhost:3000 cy:open",
    "pretest:e2e:run": "npm run build",
    "test:e2e:run": "start-server-and-test start:cli http-get://localhost:3000 cy:run",
    "test:e2e": "is-ci \"test:e2e:run\" \"test:e2e:dev\"",
    "lint": "jest --config ./jest.lint.js",
    "check-types": "tsc",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(js|jsx|ts|tsx|json|css|scss)\"",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "validate": "npm-run-all check-types format check-format lint test:jest test:e2e:run build",
    "generate": "plop",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "_moduleAliases": {
    "@babel/standalone": "node_modules/@babel/core",
    "babel-preset-env": "node_modules/@babel/preset-env"
  },
  "jest-runner-eslint": {
    "cliOptions": {
      "ignorePath": "./.gitignore"
    }
  },
  "dependencies": {
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.3.0",
    "@hookform/resolvers": "^2.8.2",
    "@mantine/core": "^3.1.0",
    "@mantine/hooks": "^3.1.0",
    "@mantine/notifications": "^3.1.0",
    "@mui/lab": "^5.0.0-alpha.53",
    "@mui/material": "^5.0.6",
    "axios": "^0.24.0",
    "clsx": "^1.1.1",
    "cuid": "^2.1.8",
    "jotai": "^1.4.2",
    "lodash.omit": "^4.5.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^7.18.0",
    "react-query": "^3.29.0",
    "react-query-auth": "^1.1.0",
    "react-router-dom": "^6.0.0-beta.8",
    "zod": "^3.11.6",
    "zustand": "^3.6.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@cypress/vite-dev-server": "^2.2.0",
    "@mswjs/data": "^0.7.0",
    "@testing-library/cypress": "^8.0.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/faker": "^5.5.9",
    "@types/jest": "^27.0.2",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/lodash.omit": "^4.5.6",
    "@types/node": "^16.11.6",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@types/react-helmet": "^6.1.4",
    "@types/react-router-dom": "^5.3.2",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "@vitejs/plugin-react": "^1.0.7",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "autoprefixer": "^10.4.0",
    "cross-env": "^7.0.3",
    "cypress": "^8.7.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "faker": "^5.5.3",
    "history": "^5.0.1",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "jest-axe": "^5.0.1",
    "jest-runner-eslint": "^1.0.0",
    "module-alias": "^2.2.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.11",
    "prettier": "^2.4.1",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^2.2.19",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4",
    "vite": "^2.6.13",
    "web-vitals": "^2.1.2"
  }
}
